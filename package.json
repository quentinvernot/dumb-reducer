{
  "name": "dumb-reducer",
  "version": "1.0.7",
  "description": "A generic redux reducer for simple cases",
  "main": "lib/index.js",
  "module": "es/index.js",
  "license": "MIT",
  "repository": "https://github.com/quentinvernot/dumb-reducer",
  "scripts": {
    "build:es": "BABEL_ENV=esnext babel -d es src",
    "build:babel": "BABEL_ENV=es5 babel -d lib src",
    "build:flow": "flow-copy-source src lib",
    "build": "yarn build:es && yarn build:babel && yarn build:flow",
    "lint:flow": "flow check",
    "lint:js": "eslint es/*.js",
    "test": "mocha \"test/*.spec.js\" --compilers js:babel-register --recursive --full-trace --reporter=spec",
    "prepare": "yarn lint:js && yarn lint:flow && yarn build"
  },
  "bin": {
    "eslint": "./node_modules/.bin/eslint",
    "flow": "./node_modules/.bin/flow",
    "mocha": "./node_modules/.bin/mocha"
  },
  "files": [
    "es",
    "lib",
    ".flowconfig"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.3",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "deep-freeze": "^0.0.1",
    "eslint": "^4.13.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha-no-only": "^0.0.5",
    "flow": "^0.2.3",
    "flow-bin": "^0.61.0",
    "mocha": "^4.0.1"
  },
  "dependencies": {
    "flow-copy-source": "^1.2.1"
  }
}
